<jyaml_plugin>

  <name>IE min/max Expressions</name>
  <author>Reinhard Hiebl</author>
  <creationDate>November 2007</creationDate>
  <copyright>(C) Reinhard Hiebl</copyright>
  <license>http://www.jyaml.de</license>
  <authorEmail>service@hieblmedia.de</authorEmail>
  <authorUrl>www.hieblmedia.de</authorUrl>
  <version>1.1</version>
  <description>YAML IE_JS_MIN_MAX DESC</description>
  
  <params>
    <param name="published" type="radio" default="0" label="Enabled" description="Enabled">
      <option value="0">No</option>
      <option value="1">Yes</option>
    </param>
    
    <param name="@spacer" type="spacer" default="" label="" description="" />
    
    <param name="selector" type="text" default="#page_margins" label="YAML IE_JS_MIN_MAX LABEL SELECTOR" description="YAML IE_JS_MIN_MAX DESC SELECTOR" />

    <param name="@spacer" type="spacer" default="" label="" description="" />
    
    <param name="width_fallback" type="text" default="" label="YAML IE_JS_MIN_MAX LABEL WIDTHFALLBACK" description="YAML IE_JS_MIN_MAX DESC WIDTHFALLBACK" />    
    <param name="min_width" type="text" default="" label="YAML IE_JS_MIN_MAX LABEL MINWIDTH" description="YAML IE_JS_MIN_MAX DESC MINWIDTH" />
    <param name="max_width" type="text" default="" label="YAML IE_JS_MIN_MAX LABEL MAXWIDTH" description="YAML IE_JS_MIN_MAX DESC MAXWIDTH" />
  </params>
  
</jyaml_plugin>